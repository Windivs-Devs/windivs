LANGUAGE LANG_ROMANIAN, SUBLANG_NEUTRAL

IDD_STARTPAGE DIALOGEX 0, 0, 317, 193
STYLE DS_SHELLFONT | DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Expert de instalare Windivs"
FONT 8, "MS Shell Dlg"
BEGIN
    LTEXT "Bun venit la instalarea Windivs", IDC_STARTTITLE, 115, 8, 195, 24
    LTEXT "Windivs încă nu poate fi instalat direct de pe acest CD! Pentru a instala Windivs, reporniți calculatorul și utilizați acest CD în secvența de inițializare.", IDC_STATIC, 115, 40, 195, 100
    LTEXT "Apăsați pe Sfârșit pentru a ieși.", IDC_STATIC, 115, 169, 195, 17
END

IDD_TYPEPAGE DIALOGEX 0, 0, 317, 143
STYLE DS_SHELLFONT | DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Expert de instalare Windivs"
FONT 8, "MS Shell Dlg"
BEGIN
    AUTORADIOBUTTON "Instalează Windivs", IDC_INSTALL, 7, 7, 277, 10, WS_GROUP | WS_TABSTOP
    LTEXT "Instalarea unei noi copii a Windivs. Fișierele, configurațiile și programele existente nu vor fi păstrate. Sunt posibile ajustări asupra discurilor și partițiilor.", IDC_INSTALLTEXT, 18, 18, 269, 25, NOT WS_GROUP
    AUTORADIOBUTTON "Repară sau actualizează Windivs", IDC_UPDATE, 7, 50, 277, 10
    LTEXT "Repararea sau actualizarea Windivs. Această opțiune menține fișierele, configurațiile și programele existente. Opțiune validă doar dacă Windivs e deja instalat în calculator.", IDC_UPDATETEXT, 18, 61, 269, 30, NOT WS_GROUP
    LTEXT "Apăsați pe Înainte pentru configurarea de dispozitive.", IDC_STATIC, 7, 128, 297, 8
END

IDD_UPDATEREPAIRPAGE DIALOGEX 0, 0, 317, 143
STYLE DS_SHELLFONT | DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Expert de instalare Windivs"
FONT 8, "MS Shell Dlg"
BEGIN
    LTEXT       "Expertul de instalare Windivs poate actualiza una dintre instalările enumerate mai jos, sau, dacă o instalare Windivs este deteriorată, să încerce să o repare.", IDC_STATIC, 6, 6, 300, 18
    CONTROL     "", IDC_NTOSLIST, "SysListView32", LVS_REPORT | LVS_SINGLESEL | LVS_SHOWSELALWAYS | LVS_ALIGNLEFT | WS_BORDER | WS_TABSTOP, 6, 30, 303, 90
    PUSHBUTTON  "N&u actualiza", IDC_SKIPUPGRADE, 230, 128, 80, 14
    LTEXT       "Apăsați Înainte pentru a actualiza instalarea selectată, sau a continua cu o nouă instalare dacă aveți bifată opțiunea „Nu actualiza”.", IDC_STATIC, 7, 124, 222, 16
END

