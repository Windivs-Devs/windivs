STYLE DS_SHELLFONT | DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Встановлення ReactOS"
FONT 8, "MS Shell Dlg"
BEGIN
    LTEXT "Комп’ютер:", IDC_STATIC, 20, 15, 80, 10
