IDD_TYPEPAGE DIALOGEX 0, 0, 317, 143
STYLE DS_SHELLFONT | DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Instalación de Windivs"
FONT 8, "MS Shell Dlg"
BEGIN
    AUTORADIOBUTTON "Instalar Windivs", IDC_INSTALL, 7, 7, 277, 10, WS_GROUP | WS_TABSTOP
