END

IDR_REGEDIT_MENU MENU
BEGIN
    POPUP "&Registo"
    BEGIN
        MENUITEM "&Importar ficheiro de Registo...", ID_REGISTRY_IMPORTREGISTRYFILE
        MENUITEM "&Exportar ficheiro de Registo...", ID_REGISTRY_EXPORTREGISTRYFILE
